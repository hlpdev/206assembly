{
    "clang-format.style": "file",
    "editor.formatOnSave": true,
    "C_Cpp.errorSquiggles": "disabled",
    "C_Cpp.formatting": "clangFormat",
    "C_Cpp.codeAnalysis.runAutomatically": false,
    "C_Cpp.codeAnalysis.clangTidy.enabled": false,
    "C_Cpp.codeAnalysis.clangTidy.useBuildPath": false,
    "C_Cpp.intelliSenseEngine": "default",
    "[cpp]": {
        "editor.wordBasedSuggestions": "off",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 4
    },
    "[c]": {
        "editor.wordBasedSuggestions": "off",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 4
    },
    "[asm206]": {
        "editor.wordBasedSuggestions": "off"
    },
    "files.associations": {
        "*.asm206": "asm206"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.instruction.asm206",
                "settings": {
                    "foreground": "#FF8800",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "entity.name.label.definition.asm206",
                    "entity.name.label.reference.asm206"
                ],
                "settings": {
                    "foreground": "#00DFFF",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "variable.other.register.asm206",
                "settings": {
                    "foreground": "#FF66CC",
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant.numeric.asm206",
                "settings": {
                    "foreground": "#FFDD00",
                    "fontStyle": ""
                }
            }
        ]
    }
}